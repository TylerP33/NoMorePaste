{
  "name": "Stackoverflow Code Highlighter",
  "version": "0.1",
  "manifest_version": 2,

  "description": "Test Extension",

  "permissions": [ "activeTab", "storage" ],

  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": { "16": "images/16.png",
           "48": "images/48.png",
          "128": "images/128.png" },
  "content_scripts" : [{
        "matches" :  ["*://*/*"],
        "js" : ["jquery.js"]
    }]
}